{
    "compilerOptions": {
        "target": "ES2022",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "allowJs": true,
        "checkJs": false,
        "noEmit": true,
        "strict": false,
        "skipLibCheck": true
    },
    "include": [
        "*.js",
        "content.js"
    ],
    "exclude": [
        "node_modules"
    ]
}
